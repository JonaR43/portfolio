{
  "name": "portfolio-backend",
  "version": "1.0.0",
  "description": "Portfolio CMS Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
